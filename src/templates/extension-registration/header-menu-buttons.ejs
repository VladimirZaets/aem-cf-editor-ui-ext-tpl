                  guestConnection.host.modal.showUrl({
                    title: "<%- button.label %>",
                    url: modalURL,
                  });
                  <%_ } else { -%>
                  console.log("Button '<%- button.id %>' has been pressed.");
                  <%_ } -%>
                },
              },
              <%_ }) -%>
            ];
          },
